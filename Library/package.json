{
  "name": "ai-models-bridge",
  "version": "0.1.0",
  "description": "A library for interacting with various AI models",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "bundle": "rollup -c rollup.config.js",
    "build:all": "npm run build && npm run bundle",
    "copy-to-demo": "node scripts/copy-to-demo.js",
    "build:demo": "npm run build:all && npm run copy-to-demo",
    "test": "jest",
    "prepublishOnly": "npm run build:all"
  },
  "keywords": [
    "ai",
    "chatbot",
    "llm",
    "chatgpt",
    "gemini",
    "claude"
  ],
  "author": "Sukarth Acharya",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.1",
    "ofetch": "^1.0.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@types/webextension-polyfill": "^0.10.0",
    "rollup": "^4.9.6",
    "typescript": "^5.0.0"
  }
}
